{
    "name": "idealista/property-based-testing-workshop",
    "type": "project",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Idealista\\PropertyBasedTestingWorkshop\\": "src/"
        },
        "files": [
            "src/functions.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Idealista\\PropertyBasedTestingWorkshop\\Tests\\": "tests/"
        }
