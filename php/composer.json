        "phpunit/phpunit": "^10.3",
        "innmind/black-box": "^5.1",
        "friendsofphp/php-cs-fixer": "^3.23",
        "phpstan/phpstan-strict-rules": "^1.5",
        "vimeo/psalm": "^5.14",
        "psalm/plugin-phpunit": "^0.18.4",
